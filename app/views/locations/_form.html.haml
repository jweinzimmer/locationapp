= form_for @location do |f|
  .container.well
    .wrapper
      %p
        = f.label :address
        %br
        = f.text_field :address
      %p
        = f.label :latitude
        %br
        = f.text_field :latitude
      %p
        =f.label :longitude
        %br
        =f.text_field :longitude
      %p
        =f.submit











